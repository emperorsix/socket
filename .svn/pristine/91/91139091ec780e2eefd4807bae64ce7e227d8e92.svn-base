package com.zhangxuan.net.socket.model;

public class TextFrame extends BinaryFrame {
	public String getText() {
		byte[] data = getData();
		if (data == null)
			return "";
		return new String(data);
	}

	public void setText(String content) {
		if (content == null)
			return;
		setData(content.getBytes());
	}
}
